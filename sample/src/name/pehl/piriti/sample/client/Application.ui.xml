<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .important {
            font-weight: bold;
        }
    </ui:style>
    <g:DockLayoutPanel unit="EM">
        <g:west size="15">
            <g:VerticalPanel>
                <g:Hyperlink ui:field="fromJsonAsPojo" targetHistoryToken="fromJsonAsPojo">JSON &rarr; POJO</g:Hyperlink>
                <g:Hyperlink ui:field="fromJsonAsGxtModel" targetHistoryToken="fromJsonAsGxtModel">JSON &rarr; GXT Model</g:Hyperlink>
                <g:Hyperlink ui:field="fromXmlAsPojo" targetHistoryToken="fromXmlAsPojo">XML &rarr; POJO</g:Hyperlink>
                <g:Hyperlink ui:field="fromXmlAsGxtModel" targetHistoryToken="fromXmlAsGxtModel">XML &rarr; GXT Model</g:Hyperlink>
            </g:VerticalPanel>
        </g:west>
        <g:center>
            <g:ScrollPanel>
                <g:Label ui:field="sourceCode"/>
            </g:ScrollPanel>
        </g:center>
        <g:south size="2">
            <g:HTML>
                <div>
                    Read <span ui:field="bookCount">n/a</span> books in <span ui:field="interval">n/a</span> ms.
                </div>
            </g:HTML>
        </g:south>
    </g:DockLayoutPanel>
</ui:UiBinder> 