#parse("name/pehl/piriti/rebind/json/writer/property/commons.vm")
if ($jsonValue != null)
{
    if (${jsonValue}.isNull() == null) 
    {
        JSONNumber jsonNumber = ${jsonValue}.isNumber();
        if (jsonNumber != null)
        {
            Double doubleValue = new Double(jsonNumber.doubleValue());
            #valueOfNumeric("doubleValue", $value, $property.type)
        }
        else
        {
	        JSONString jsonString = ${jsonValue}.isString();
	        if (jsonString != null)
	        {
	            String stringValue = jsonString.stringValue();
	            if (stringValue != null)
	            {
	               #convert("stringValue")
	            }
	        }
        }
    }
	if ($value != null) 
    {
        #assignValue()
	}
}
