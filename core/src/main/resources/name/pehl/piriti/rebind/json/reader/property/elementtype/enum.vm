JSONString jsonString = currentJsonValue.isString();
if (jsonString != null)
{
    String stringValue = jsonString.stringValue();
    if (stringValue != null)
    {
        if ($converter != null)
        {
            $currentValue = ${converter}.convert(stringValue);
        }
        else
        {
            boolean error = true;
            String[] stringValues = new String[]{stringValue, stringValue.toUpperCase(), stringValue.toLowerCase()};
            for (String s : stringValues)
            {
                try 
                {
                    $currentValue = ${elementType}.valueOf(s);
                    error = false;
                    break;
                }
                catch (IllegalArgumentException e) {} 
            }
            if (error)
            {
                if (logger.isLoggable(SEVERE)) 
                {
                    logger.log(SEVERE, "Cannot convert any of " + stringValues + " to enum $parameterizedValueType $property.name in ${modelType} using ${elementType}.valueOf(String).");
            }
        }
    }
}
